{
  "name": "@codelab/api",
  "license": "ISC",
  "main": "index.js",
  "types": "__generated__/types/index.d.ts",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm-run-all --parallel nodemon watch:types generate:graphql-types",
    "nodemon": "nodemon",
    "start": "concurrently --names Types,Scripts --prefix-colors blue,yellow \"yarn watch:types\" \"yarn watch:scripts\" --color",
    "watch:types": "ttsc -w --project tsconfig.build.json --emitDeclarationOnly --declaration true --declarationMap true",
    "watch:scripts": "webpack --watch",
    "build": "yarn build:scripts",
    "build:types": "ttsc --project tsconfig.build.json --emitDeclarationOnly --declaration true --declarationMap true --resolveJsonModule",
    "build:scripts": "webpack",
    "lint": "yarn tslint && yarn prettier",
    "prettier": "prettier --config ../../.prettierrc.yml \"**/!(types)/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore --write",
    "tslint": "tslint --project tsconfig.json \"?(src|stories|tests)/**/*.{ts,tsx}\" --fix",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "generate:graphql-types": "graphql-codegen --watch --require dotenv/config --config codegen.yml"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "@types/koa-router": "^7.0.40",
    "@types/mongoose": "^5.3.10",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-multer": "^1.0.2",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.11.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "mongoose": "^5.0.17",
    "mongoose-unique-array": "^0.2.0",
    "p-iteration": "^1.1.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sinon": "^7.2.3",
    "sinon-mongoose": "^2.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.4",
    "@graphql-codegen/cli": "^1.4.0",
    "@graphql-codegen/typescript": "^1.4.0",
    "@graphql-codegen/typescript-operations": "^1.4.0",
    "@jest/test-sequencer": "^24.8.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/jquery": "^3.3.29",
    "@types/koa": "^2.0.48",
    "@types/node": "^10.0.8",
    "@zerollup/ts-transform-paths": "^1.7.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "clean-webpack-plugin": "^2.0.1",
    "concurrently": "^4.1.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "gulp": "^3.9.1",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mongodb-memory-server": "^5.1.4",
